<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <!-- set your key to User.config file or directly -->
    <appSettings file="User.config">
        <!-- BigQuery -->
        <add key="certificateFileName" value="xxx.p12" />
        <add key="certificatePassword" value="notasecret" />
        <add key="serviceAccountEmail" value="xxx@developer.gserviceaccount.com" />
        <add key="applicationName" value="StoreTwitterTimeline" />
        <add key="projectId" value="xxx" />
        <!-- Twitter -->
        <add key="consumerKey" value="xxx" />
        <add key="consumerSecret" value="xxx" />
        <add key="accessToken" value="xxx" />
        <add key="accessSecret" value="xxx" />
    </appSettings>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <runtime>
        <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
            <dependentAssembly>
                <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
                <bindingRedirect oldVersion="0.0.0.0-7.0.0.0" newVersion="7.0.0.0" />
            </dependentAssembly>
            <dependentAssembly>
                <assemblyIdentity name="System.Net.Http.Primitives" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
                <bindingRedirect oldVersion="0.0.0.0-4.2.29.0" newVersion="4.2.29.0" />
            </dependentAssembly>
        </assemblyBinding>
    </runtime>

</configuration>